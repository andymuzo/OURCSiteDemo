body {
	width: 100%;
	background-color: #666;
	font-family: "Calibri", sans-serif;
}

img {
	margin: 0;
}

.main_page {
	width: 800px;
	margin-right: auto;
	margin-left: auto;
	border-width: 1px;
	border-style: solid;
}

.page_head {
	position: relative;
	padding: 0;
	margin: 0;
	overflow: hidden;
}

.title_bar {
	background-color: #fff;
	position: relative;
	padding: 0;
	margin: 0;
	overflow: hidden;
}

.ou_logo {
	float: left;
	width: 100px;
	height: 100px;
	padding: 0;
	margin: 0;
}

h1 {
	font-size: 60px;
	margin: 0;
	padding: 10px;
}

.page_title {
	position: absolute;
	left: 100px;

}

.login_info {
	float: right;
	padding: 10px;
}

.login_info p, .login_info a {
	margin: 2px;
}

.nav_bar {
	width: 800px;
	margin-top: -15px;
	background-color: #bbb;
	overflow: hidden;
}

ul.navigation {
	margin-right: auto;
	margin-left: auto;
	width: 450px;
	list-style: none;
 	padding: 0;
}

ul.navigation li {
	display: inline;
	margin-right: auto;
	margin-left: auto;
}

ul.navigation li :hover {
	background-color: #ccc;
}

ul.navigation li a {
	text-decoration: none;
	padding: 5px 0;
	width: 33%;
	text-align: center;
	float: left;
	color: #000;
	border-right: 1px solid #ccc;
	margin-bottom: 1px;
	background-color: #ddd;
}

ul.navigation li:first-child a {
	border-left: 1px solid #ccc;
}

#active_nav_element {
	background-color: #fff;
}

.page_content {
	position: relative;
	background-color: #fff;
}

.content_title {
	width: 100%;
	
	background-color: #CAF010;
}

h2 {
	font-size: 40px;
	width: 200px;
	text-align: center;
	margin-right: auto;
	margin-left: auto;
	margin-top: 0;
	margin-bottom: 0;
	padding-top: 10px;
}

.horizontal_bar {
	width: 100%;
	margin: 0;
	padding: 0;
	display: block;
}

.main_content {
	float: right;
	width: 780px;
	padding: 10px;
}

.main_content table th, .main_content table td {
	padding-left: 8px;
	padding-right: 8px;
	border: 1px solid;
	border-collapse: collapse;
	margin: 0;
}

.main_content table {
	width: 100%;
}

.content_footer {
	width: 100%;
	padding: 8px;
	clear: both;
}

.tabs_container {
	width: 100%;
	overflow: hidden;
	margin-top: 0;
	background-color: #bbb;
}

ul.tabs {
	list-style: none;
 	padding: 0;
 	margin-top: 5px;

}

ul.tabs li {
	display: inline;
	border-top-left-radius: 10px;
	border-top-right-radius: 10px;
}

ul.tabs li a:hover {
	background-color: #ccc;
}

ul.tabs li a {
	text-decoration: none;
	padding: 5px;
	min-width: 100px;
	text-align: center;
	float: left;
	color: #000;
	margin-bottom: 0px;
	border-top-left-radius: 10px;
	border-top-right-radius: 10px;
	margin-left: 5px;
	background-color: #ddd;
}


#active_tab {
	background-color: #fff;
}

input {
	margin: 3px 0px 3px 0px;
}

.wide_input {
	width: 300px;
}

.edit_member_form {
	float: left;
	padding: 0 8px 0 8px;
	background-color: #eee;
	margin: 10px 0 10px 0;
}

.members_list {
	margin-left: auto;
	margin-right: auto;
	background-color: #eee;
	padding: 8px;
	clear: all;
}

.message_members_form {
	background-color: #eee;
	padding: 8px;
}

.message_members_form textarea {
	width: 756px;
	height: 500px;
}